{
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"eslint": "^9.36.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"prettier": "^3.6.2"
	,
"lint-staged": {
  "**/*.{ts,tsx,js,jsx}": [
